{% extends 'base.html.twig' %}
{% block title %}Liste des produits de la catégorie {{ category.name }}{% endblock %}
{% block body %}
  <main class="container">
    <section class="row">
      <div class="col-12">
        <h1>Liste des produits de la catégorie {{ category.name }}</h1>
      </div>
      {% for product in category.products %}
        {{dump(product)}}
      {% endfor %}
    </section>
  </main>
{% endblock %}